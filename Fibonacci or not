#include<stdio.h>
#include<stdbool.h>
int main(){
    int n;
    scanf("%d",&n);
    int a=0,b=1,c=0;
    bool flag=false;
    while(n!=c)
    {
        c=a+b;
        b=a;
        a=c;
        if(c==n) {
         flag=true;
         break;
        }
       if(n<c) {
           flag=false;
           break;
       }
    }
    if(flag==true) printf("True");
      else printf("False");
   
    
}
